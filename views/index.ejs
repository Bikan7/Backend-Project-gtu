<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= data.title %></title>
  </head>
  <body>
    <br />
    <div>
      <% data.data.forEach(element => { %>
      <h3><%= element.name %></h3>
      <br />
      <% element.books.forEach(item=>{ %>
      <img src="<%=item.imgUrl%>" width="150px" height="220px" />
      <p><%=item.name%></p>
      <%}) %>
      <hr />
      <% }); %>
    </div>
  </body>
</html>
